[  
  {
    "kernelSource": {
      "name": "GT 5.4",
      "repo": "https://github.com/realme-kernel-opensource/realme_GT_GT2-AndroidS-kernel-source",
      "branch": "master",
      "device": "RMX2202",
      "defconfig": "RMX2202_defconfig"
    },
    "withKernelSU": false,
    "toolchains": [
{
        "repo": "https://github.com/kdrag0n/proton-clang",
        "branch": "master",
        "name": "proton-clang",
        "binPath": ["./bin"]
      },
    ],
    "ccache": true,
"params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommand": {
        "make -j$(nproc --all)",
        "O=out",
        "ARCH=arm64",
        "CC=clang",
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "proton-clang/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "proton-clang/bin/arm-linux-gnueabi-",
      }
    },
     "AnyKernel3": {
      "use": false,
      "release": true,
      "repo": "https://github.com/easterNday/AnyKernel3/",
      "branch": "sm8350"
    }
]
